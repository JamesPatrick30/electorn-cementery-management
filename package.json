{
  "name": "cementery-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:electron:watch": "tsc -p tsconfig.electron.json --watch",
    "electron": "electron electron-dist/main.cjs",
    "start": "concurrently \"npm run dev\" \"npm run build:electron:watch\" \"npm run electron\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "firebase": "^12.9.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
